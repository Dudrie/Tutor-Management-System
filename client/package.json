{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "main": "src/index.tsx",
  "proxy": "http://localhost:8080",
  "scripts": {
    "start": "craco start",
    "build": "cross-env NODE_ENV=production craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "format": "yarn --cwd ../ eslint --fix \"client/src/**/*.ts?(x)\"",
