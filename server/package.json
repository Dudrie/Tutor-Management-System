{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist temp",
    "build": "nest build --path \"./tsconfig.build.json\"",
    "format": "eslint --fix \"./**/*.ts?(x)\"",
    "lint": "pnpm run eslint:check",
    "eslint:check": "eslint \"./**/*.ts?(x)\"",
    "prettier:check": "prettier --check \"./**/*.ts?(x)\"",
    "prettier:format": "prettier --write --loglevel warn \"./**/*.ts?(x)\"",
    "ts:check": "tsc --project tsconfig.check.json --noEmit --incremental false",
    "ts:check:watch": "tsc --project tsconfig.check.json --noEmit --incremental false -w",
    "start": "env-cmd cross-env NODE_ENV=development nest start -p ./tsconfig.dev.json --debug --watch",
    "start:timezone": "cross-env TZ='Europe/Berlin' pnpm start",
    "start:nowatch": "env-cmd cross-env NODE_ENV=development nest start",
    "start:prod": "node dist/main",
    "test": "env-cmd -f ./.env.test cross-env NODE_ENV=test jest --verbose=true",
    "test:watch": "pnpm test --watch",
    "test:cov": "pnpm test --coverage",
    "test:debug:win": "env-cmd -f ./.env.test cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register ../node_modules/jest/bin/jest.js --runInBand",
    "derp": "jest --init",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@mikro-orm/core": "^4.5.7",
    "@mikro-orm/mysql": "^4.5.7",
    "@mikro-orm/nestjs": "^4.2.0",
    "@mikro-orm/reflection": "^4.5.7",
    "@nestjs/common": "^8.0.5",
    "@nestjs/core": "^8.0.5",
    "@nestjs/passport": "^8.0.0",
    "@nestjs/platform-express": "^8.0.5",
    "@nestjs/swagger": "^5.0.9",
    "@types/uuid": "^8.3.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "class-validator": "^0.12.2",
    "excel4node": "^1.7.2",
    "express-session": "^1.17.1",
    "highlight.js": "^10.7.2",
    "jszip": "^3.5.0",
    "luxon": "^1.25.0",
    "nodemailer": "^6.4.17",
    "papaparse": "^5.3.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pug": "^3.0.0",
    "puppeteer": "5.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.0",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/testing": "^8.0.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.24",
    "@types/express-session": "^1.17.0",
    "@types/jest": "^26.0.24",
    "@types/luxon": "^1.25.1",
    "@types/node": "^16.4.7",
    "@types/nodemailer": "^6.4.0",
    "@types/papaparse": "^5.2.4",
    "@types/passport-local": "^1.0.34",
    "@types/pug": "^2.0.4",
    "@types/puppeteer": "5.4.2",
    "@types/supertest": "^2.0.11",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "supertest": "^6.1.4",
    "ts-jest": "^27.0.4",
    "ts-loader": "^9.2.4",
    "tsconfig-paths": "^3.10.1"
  }
}
