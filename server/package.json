{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "description": "Server implementation of the TMS",
  "main": "build/server.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc -b",
    "postbuild": "copyfiles -E \"config/**/*\" build/",
    "build:watch": "tsc -b -w",
    "start": "ts-node-dev --no-notify . --no-lazy",
    "test": "echo \"Error: no test specified\" && exit 0",
    "eslint:check": "eslint \"src/**/*.ts?(x)\"",
    "format": "eslint --fix \"src/**/*.ts?(x)\"",
    "prettier:check": "prettier --check \"src/**/*.ts?(x)\"",
    "ts:check": "tsc --project . --noEmit"
  },
  "dependencies": {
    "@hasezoey/typegoose": "^5.9.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "config": "^3.2.2",
    "connect-mongo": "^3.0.0",
    "express": "^4.17.1",
    "express-promise-router": "^3.0.3",
    "express-session": "^1.16.2",
    "googleapis": "^43.0.0",
    "html-pdf": "^2.2.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.1",
    "mongoose-field-encryption": "^3.0.0",
    "nodemailer": "^6.3.0",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "reflect-metadata": "^0.1.13",
    "shared": "1.0.0",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "^0.0.34",
    "@types/connect-mongo": "^0.0.43",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.14",
    "@types/html-pdf": "^2.2.0",
    "@types/mongoose": "^5.5.18",
    "@types/nodemailer": "^6.2.1",
    "@types/passport": "^1.0.1",
    "@types/passport-http": "^0.3.8",
    "copyfiles": "^2.1.1",
    "ts-node-dev": "^1.0.0-pre.42"
  }
}
