{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "description": "Server implementation of the TMS",
  "main": "build/server.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc -b",
    "build:watch": "tsc -b -w",
    "prestart": "yarn build",
    "start": "nodemon .",
    "test": "echo \"Error: no test specified\" && exit 0",
    "eslint:check": "eslint \"src/**/*.ts?(x)\"",
    "eslint:fix": "eslint --fix \"src/**/*.ts?(x)\"",
    "prettier:check": "prettier --check \"src/**/*.ts?(x)\"",
    "ts:check": "tsc -b --force && tsc --project . --noEmit"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.0.0",
    "express": "^4.17.1",
    "express-promise-router": "^3.0.3",
    "express-session": "^1.16.2",
    "mongoose": "^5.6.9",
    "mongoose-encryption": "^2.0.2",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "reflect-metadata": "^0.1.13",
    "shared": "1.0.0",
    "typegoose": "^5.9.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-mongo": "^0.0.43",
    "@types/express": "^4.17.0",
    "@types/express-session": "^1.15.13",
    "@types/mongoose": "^5.5.12",
    "@types/passport": "^1.0.0",
    "@types/passport-http": "^0.3.8"
  }
